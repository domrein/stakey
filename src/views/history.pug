extends page.pug

append head
  style.
    tbody tr:nth-child(even) {
      background-color: rgba(200, 200, 200, 0.5);
    }
    th {
      width: 25%;
    }

    table {
      border-collapse: collapse
    }

append content
  h3 #{stake}

  h1 History
  table(style="width: 100%; text-align: center;")
    tr
      th Candidate
      th Position
      th State
      th Deleted
    each calling in callings
      tr
        td #{calling.name}
        td #{calling.position}
        td #{calling.state}
        td #{calling.deleted}